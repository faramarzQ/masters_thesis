# App env vars
FIBONACCI_NODEPORT_SERVICE_HOST=127.0.0.1
FIBONACCI_NODEPORT_SERVICE_PORT=3333

IMAGE_NAME=gateway

run-gateway:
	FIBONACCI_NODEPORT_SERVICE_HOST=$(FIBONACCI_NODEPORT_SERVICE_HOST) \
	FIBONACCI_NODEPORT_SERVICE_PORT=$(FIBONACCI_NODEPORT_SERVICE_PORT) \
	go run main.go

build-gateway-image:
	docker build --tag $(IMAGE_NAME):1.0 .